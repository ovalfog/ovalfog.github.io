    <title>📕关于异位同因论</title>
    <link href="http://example.com/2022/11/12/%F0%9F%93%95%E5%85%B3%E4%BA%8E%E5%BC%82%E4%BD%8D%E5%90%8C%E5%9B%A0%E8%AE%BA/"/>
    <id>http://example.com/2022/11/12/%F0%9F%93%95%E5%85%B3%E4%BA%8E%E5%BC%82%E4%BD%8D%E5%90%8C%E5%9B%A0%E8%AE%BA/</id>
